{
    "version": 2,
    "env": {
      "POSTGRES_URL": "${PROD_POSTGRES_HOST}", 
      "POSTGRES_URL_NON_POOLING": "${PROD_PGHOST_UNPOOLED}",
      "JWT_SECRET": "${PROD_JWT_SECRET}"
    },
    "builds": [
      {
        "src": "server.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ]
}