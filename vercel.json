{
    "version": 2,
    "env": {
      "POSTGRES_URL": "PROD_POSTGRES_URL",
      "POSTGRES_URL_NON_POOLING": "PROD_POSTGRES_URL_NON_POOLING",
      "JWT_SECRET": "PROD_JWT_SECRET"
    },
    "builds": [
      {
        "src": "server.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.mjs"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)\\.(?:js|css|svg|png|jpg|gif|ico)$",
        "dest": "/dist/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }