{
  "version": 2,
  "builds": [
    {
      "src": "setup-db.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.mjs"
    },
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ],
  "buildCommand": "node setup-db.mjs && npm run build"
}