{
    "env": {
      "POSTGRES_URL": "@postgres_url",
      "POSTGRES_URL_NON_POOLING": "@postgres_url_non_pooling",
      "JWT_SECRET": "@jwt_secret"
    },
    "builds": [
      {
        "src": "server.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.mjs"
      }
    ]
  }